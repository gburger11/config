!Remap all modifiers
clear lock
clear shift
clear mod5
remove shift = Shift_L
remove shift = Shift_R
keycode 66Â = ISO_Level3_Shift ISO_Level3_Shift ISO_Level3_Lock ISO_Level3_Lock
keycode 51 = ISO_Level3_Shift ISO_Level3_Lock ccedilla Ccedilla
keycode 49 = Mode_switch
keycode 50 = Shift_L Caps_Lock
keycode 62 = Shift_R Caps_Lock
add lock = Caps_Lock
add shift = Shift_L Shift_R
add mod5 = ISO_Level3_Shift

